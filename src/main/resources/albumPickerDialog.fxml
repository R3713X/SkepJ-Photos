<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.DatePicker?>

<?import javafx.scene.control.ChoiceBox?>
<DialogPane fx:controller="UI.AlbumPickerDialogController"
            xmlns:fx="http://javafx.com/fxml">
    <headerText>
        Choose the album:
    </headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Album Title" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <ChoiceBox fx:id="albumChoiceBox" GridPane.rowIndex="0" GridPane.columnIndex="1"></ChoiceBox>

        </GridPane>
    </content>
</DialogPane>